   <string notr="true">QMainWindow#MainWindow {
    background-color: #1E1E1E;   /* Dark gray background for the main window */
}

QPushButton {
    border-radius: 8px;              /* Rounded corners */
    border: 0.5px solid #434343;  /* Button border */
    background-color: #3C3C3C;        /* Button background color */
    color: #FFFFFF;                   /* Text color */
    padding: 0px;                    /* Internal padding */
}


QPushButton {
    margin: 5px;                      /* External margin between buttons */
}

QPushButton:hover {
    border: 1px solid #414141;  /* Change border color on hover */
}

QPushButton:pressed {
    border: 1px solid #3a3a3a;  /* Change border color on press */
}

/* Special style for QPushButton with objectName 'buttonEquals' */
QPushButton#buttonEquals {
	background-color: #DB9EE5;  /* Custom background for buttonEquals */
	color: #000000;  /* Black text color for buttonEquals */
	border: 1px solid #dea5e7;  /* White border for normal state */
}

QPushButton#buttonEquals:hover {
    border: 1px solid #a97eaf;  /* White border on hover */
}

QPushButton#buttonEquals:pressed {
    border: 1px solid #a97eaf;  /* White border on press */
}
